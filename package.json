{
  "name": "chat-tool-server",
  "version": "1.0.0",
  "description": "Chat management tool",
  "repository": "https://github.com/thaihuynhatquang/chat-tool.git",
  "main": "src/bin/www.js",
  "nodemonConfig": {
    "ignore": [
      "/app/data/*"
    ]
  },
  "scripts": {
    "start": "npm run migrate && npm run seed && if-env NODE_ENV=local && npm run start:dev || npm run start:prod",
    "start:dev": "DEBUG=app* nodemon src/bin/www.js --exec babel-node",
    "start:prod": "npm run build && npm run serve",
    "build": "babel src/ -d dist/",
    "serve": "pm2-runtime process.yml",
    "migrate": "sequelize db:migrate",
    "migrate:flush": "sequelize db:migrate:undo:all",
    "test": "eslint src && flow",
    "seed": "sequelize db:seed:all",
    "seed:flush": "sequelize db:seed:undo:all",
    "generate-apidoc": "apidoc -i src/routes/api/v1/ -o apidoc/",
    "generate-flow-report": "flow-coverage-report -i 'src/**/*.js' -x 'src/routes/api/v1/apiSuccessExample/**' -x 'src/database/**/*' -x 'src/types/**/*' -t html -t json -t text --threshold 70"
  },
  "author": "thaihuynhatquang@gmail.com",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "axios": "^0.18.0",
    "babel-core": "^7.0.0-bridge.0",
    "bluebird": "^3.5.2",
    "bootbot": "^1.0.16",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "cron": "^1.6.0",
    "dotenv": "^6.2.0",
    "express": "^4.17.3",
    "flow-typed": "^2.5.1",
    "form-data": "^3.0.0",
    "helmet": "^3.13.0",
    "humps": "^2.0.1",
    "if-env": "^1.0.4",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "multer": "^1.4.1",
    "mysql2": "^1.6.1",
    "redis": "^2.8.0",
    "sequelize": "^4.38.0",
    "sequelize-cli": "^4.0.0",
    "socket.io": "^2.2.0",
    "socket.io-redis": "^5.2.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-flow": "^7.0.0",
    "@babel/register": "^7.0.0",
    "apidoc": "^0.17.7",
    "babel-plugin-module-resolver": "^3.1.1",
    "nodemon": "^1.18.3",
    "prettier": "^1.15.3",
    "prettier-standard": "^8.0.1",
    "regenerator-runtime": "^0.12.1"
  }
}
