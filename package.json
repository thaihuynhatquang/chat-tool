{
  "name": "chat-tool-server",
  "version": "1.0.0",
  "description": "Chat management tool",
  "repository": "https://github.com/thaihuynhatquang/chat-tool.git",
  "main": "app.js",
  "moduleRoots": [
    "./src"
  ],
  "nodemonConfig": {
    "ignore": [
      "/app/data/*"
    ]
  },
  "scripts": {
    "start": "npm run migrate && if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:dev": "npm run develop",
    "start:prod": "npm run test && npm run lint && npm run flow && npm run build && npm run serve",
    "develop": "DEBUG=app:* nodemon src/bin/www --exec 'node --require babel-register'",
    "build": "babel src/ -d dist/",
    "serve": "node dist/app.js",
    "migrate": "sequelize db:migrate",
    "migrate:flush": "sequelize db:migrate:undo:all",
    "seed": "sequelize db:seed:all",
    "test": "jest"
  },
  "author": "thaihuynhatquang@gmail.com",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "bluebird": "^3.5.2",
    "bootbot": "^1.0.16",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "helmet": "^3.13.0",
    "if-env": "^1.0.4",
    "jsonwebtoken": "^8.3.0",
    "moment": "^2.22.2",
    "multer": "^1.4.1",
    "mysql2": "^1.6.1",
    "query-string": "^6.12.1",
    "redis": "^2.8.0",
    "sequelize": "^4.38.0",
    "sequelize-cli": "^4.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.4.2",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "jest": "^23.5.0",
    "nodemon": "^1.18.3",
    "prettier": "^1.14.3",
    "prettier-standard": "^8.0.1",
    "regenerator-runtime": "^0.12.1"
  }
}
