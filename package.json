{
  "name": "chat-tool-server",
  "version": "1.0.0",
  "description": "Chat management tool",
  "repository": "https://github.com/thaihuynhatquang/chat-tool.git",
  "main": "app.js",
  "nodemonConfig": {
    "ignore": [
      "/app/data/*"
    ]
  },
  "scripts": {
    "start": "npm run migrate && npm run seed && if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:dev": "DEBUG=app* nodemon src/bin/www.js --exec babel-node",
    "start:prod": "npm run test && npm run lint && npm run flow && npm run build && npm run serve",
    "build": "babel src/ -d dist/",
    "serve": "node dist/app.js",
    "migrate": "sequelize db:migrate",
    "migrate:flush": "sequelize db:migrate:undo:all",
    "seed": "sequelize db:seed:all",
    "test": "jest"
  },
  "author": "thaihuynhatquang@gmail.com",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "bluebird": "^3.5.2",
    "bootbot": "^1.0.16",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "googleapis": "^50.0.0",
    "helmet": "^3.13.0",
    "if-env": "^1.0.4",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "multer": "^1.4.1",
    "mysql2": "^1.6.1",
    "query-string": "^6.12.1",
    "redis": "^2.8.0",
    "sequelize": "^4.38.0",
    "sequelize-cli": "^4.0.0",
    "socket.io": "^2.2.0",
    "socket.io-redis": "^5.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "babel-plugin-module-resolver": "^3.1.1",
    "jest": "^23.5.0",
    "nodemon": "^1.18.3",
    "prettier": "^1.14.3",
    "prettier-standard": "^8.0.1"
  }
}
